namespace BasicStackOperations
{
    using System;
    using System.Linq;
    using System.Collections.Generic;

    class Program
    {
        static void Main(string[] args)
        {
            int[] commandsLine = Console.ReadLine()
                .Split(' ')
                .Select(int.Parse)
                .ToArray();

            int numbersToPush = commandsLine[0];
            int numbersToPop = commandsLine[1];
            int searchedNumber = commandsLine[2];

            int[] inputNumbersToPush = Console.ReadLine()
                .Split(' ')
                .Select(int.Parse)
                .ToArray();

            Queue<int> queue = new Queue<int>();

            for (int i = 0; i < inputNumbersToPush.Length; i++)
            {
                queue.Enqueue(inputNumbersToPush[i]);
            }

            if (numbersToPop <= queue.Count)
            {
                for (int i = 0; i < numbersToPop; i++)
                {
                    queue.Dequeue();
                }
            }
            else
            {
                while (queue.Count > 0)
                {
                    queue.Dequeue();
                }
            }

            if (queue.Contains(searchedNumber))
            {
                Console.WriteLine("true");
            }
            else
            {
                if (queue.Count == 0)
                {
                    Console.WriteLine("0");
                }
                else
                {
                    Console.WriteLine(queue.Min());
                }
            }
        }
    }
}



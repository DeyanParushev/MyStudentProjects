namespace CustomTuple
{
    using System.Text;

    public class GenericTuple<T, T2>
    {
        private T item1;
        private T2 item2;

        public GenericTuple(T item1, T2 item2)
        {
            this.Item1 = item1;
            this.Item2 = item2;

        }

        public T Item1
        {
            get { return this.item1; }
            set { this.item1 = value; }
        }

        public T2 Item2
        {
            get { return this.item2; }
            set { this.item2 = value; }
        }

        public string Print()
        {
            StringBuilder sb = new StringBuilder();

            sb.Append($"{this.Item1} -> {this.Item2}");

            return sb.ToString();
        }
    }
}
